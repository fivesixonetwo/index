{
  "name": "airbnb",
  "version": "1.0.0",
  "description": "airbnb app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/home/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/home/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Tien",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "moment": "^2.18.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-with-direction": "^1.3.1"
  }
}
